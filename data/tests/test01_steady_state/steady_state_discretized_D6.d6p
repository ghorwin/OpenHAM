			<Reference>Inside</Reference>
			<Range>57 0 57 0</Range>
		</Assignment>

		<Assignment type="Interface" location="Left">
			<Reference>Outside</Reference>
			<Range>0 0 0 0</Range>
		</Assignment>

		<Assignment type="Output" location="Element">
			<Reference>Tfield</Reference>
			<Range>0 0 57 0</Range>
		</Assignment>
	</Assignments>

